{
  "name": "vuetify-sonner",
  "type": "module",
  "version": "0.3.8",
  "packageManager": "pnpm@8.12.1",
  "description": "Stackable toast component for Vuetify.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vuetify-sonner#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vuetify-sonner.git"
  },
  "bugs": "https://github.com/wobsoriano/vuetify-sonner/issues",
  "keywords": [
    "vuetify",
    "vue",
    "confirm",
    "snackbar",
    "toast"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vuetify-sonner.js",
      "require": "./dist/vuetify-sonner.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/vuetify-sonner.umd.cjs",
  "module": "./dist/vuetify-sonner.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc --noEmit && vite build --mode lib",
    "dev": "vite serve",
    "dev:build": "vite build",
    "dev:preview": "vite preview",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "vuetify": "^3.3.0"
  },
  "dependencies": {
    "vue-sonner": "^1.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@mdi/font": "^7.3.67",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.69.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-vuetify": "^2.0.1",
    "vue": "^3.3.11",
    "vue-tsc": "^1.8.25",
    "vuetify": "^3.4.7"
  }
}
